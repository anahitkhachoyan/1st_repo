def set_voltage(self, voltage):
        # Преобразуем напряжение в число
        number = int((voltage / self.dynamic_range) * 4095)
        
        # Устанавливаем число
        self.set_number(number)
        
        if self.verbose:
            print(f"Напряжение: {voltage}V -> Число: {number}")
